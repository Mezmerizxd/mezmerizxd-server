        "express": "^4.18.1",
        "mongodb": "^4.8.1",
        "mysql2": "^2.3.3",
        "socket.io": "^4.5.1",
        "ts-loader": "^9.3.1",
        "webpack": "^5.73.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/node": "^18.6.3",
        "concurrently": "^7.3.0",
        "nodemon": "^2.0.19",
        "prettier": "^2.7.1",
        "typescript": "^4.7.4",
        "webpack-cli": "^4.10.0"
    }
}
