{
    "name": "another-express-boilerplate",
    "version": "1.0.4",
    "author": "Rory Pearson (Mezmerizxd)",
    "description": "another-express-boilerplate",
    "license": "MIT",
    "main": "dist",
    "scripts": {
        "build": "npx tsc",
        "start": "node dist/index.js",
        "watch": "webpack --mode development --watch true",
        "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
